  <ItemGroup>
    <Page Remove="UserControls\Movement_Window.xaml" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.8.4" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Projects.Commons\Projects.Commons.csproj" />
