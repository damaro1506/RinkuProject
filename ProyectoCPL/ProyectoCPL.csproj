  </ItemGroup>

  <ItemGroup>
    <Compile Update="UserControls\Movements_Window.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="UserControls\MovementsUC.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="UserControls\Movements_Window.xaml">
